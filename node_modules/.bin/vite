#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hooksvue/Desktop/vue3 h5 player/node_modules/.pnpm/vite@6.3.5_sass@1.89.1/node_modules/vite/bin/node_modules:/Users/hooksvue/Desktop/vue3 h5 player/node_modules/.pnpm/vite@6.3.5_sass@1.89.1/node_modules/vite/node_modules:/Users/hooksvue/Desktop/vue3 h5 player/node_modules/.pnpm/vite@6.3.5_sass@1.89.1/node_modules:/Users/hooksvue/Desktop/vue3 h5 player/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/hooksvue/Desktop/vue3 h5 player/node_modules/.pnpm/vite@6.3.5_sass@1.89.1/node_modules/vite/bin/node_modules:/Users/hooksvue/Desktop/vue3 h5 player/node_modules/.pnpm/vite@6.3.5_sass@1.89.1/node_modules/vite/node_modules:/Users/hooksvue/Desktop/vue3 h5 player/node_modules/.pnpm/vite@6.3.5_sass@1.89.1/node_modules:/Users/hooksvue/Desktop/vue3 h5 player/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
